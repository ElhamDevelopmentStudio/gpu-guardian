{
  "daemon_api": {
    "default_base_url": "http://127.0.0.1:8090/v1",
    "env_var": "GUARDIAN_DAEMON_BASE_URL",
    "api_version": "v1",
    "routes": {
      "health": {
        "method": "GET",
        "path": "/v1/health"
      },
      "metrics": {
        "method": "GET",
        "path": "/v1/metrics"
      },
      "sessions": {
        "method": "GET",
        "path": "/v1/sessions"
      },
      "session": {
        "method": "GET",
        "path": "/v1/sessions/{session}"
      },
      "session_telemetry": {
        "method": "GET",
        "path": "/v1/sessions/{session}/telemetry"
      },
      "start_session": {
        "method": "POST",
        "path": "/v1/sessions"
      },
      "stop_session": {
        "method": "POST",
        "path": "/v1/sessions/{session}/stop"
      },
      "control": {
        "method": "POST",
        "path": "/v1/control"
      }
    }
  }
}
